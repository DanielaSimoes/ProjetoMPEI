fileID = fopen('texto1.txt');
text1 = textscan(fileID, '%s', 'Delimiter', ' ');
fclose(fileID);

fileID = fopen('texto2.txt');
text2 = textscan(fileID, '%s', 'Delimiter', ' ');
fclose(fileID);
class(text2{1}(1))

X = initialize(1e6);

for n1 = 1:length(text2)
    for n2 = 1:length(text2{n1}) 
       BloomFilter = insert(X, char(text2{n1}(n2),3);
    end
end

counterOfSimilarWords = 0;
for n1 = 1:length(text1)
    for n2 = 1:length(text1{n1}) 
       if isMember(BloomFilter,char(text1{n1}(n2)),3);
           counterOfSimilarWords = counterOfSimilarWords + 1;
       end
    end
end

fprintf('Existem %d palavras similares.', counterOfSimilarWords);